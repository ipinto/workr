<%- def home(jobs) -%>
    <%- layouts__base do -%>
        <b>Jobs: </b>
        <ul>
            <%- jobs.each do |job_info| -%>
            <li>
                <a href="/job/<%= job_info.name %>">
                    <%= job_info.name %>
                </a>
            </li>
            <%- end -%>
        </ul>
    <%- end -%>
<%- end -%>